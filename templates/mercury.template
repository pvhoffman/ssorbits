	<td class="row-label">Phase Percent:</td>
	<td class="row-value"><span id="phase-percent"></span>%</td>
<tr>

	<td class="row-label">Magnitude:</td>
	<td class="row-value"><span id="magnitude"></span></td>
</tr>
{% endblock %}

