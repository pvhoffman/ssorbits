{% extends "base.template" %}
{% block objectID %}solarSystemOrbits.orbitalElementsMercury{% endblock %}
{% block header %}
Approximate Mercury Orbital Data
{% endblock %}
{% block ssphase %}
<tr>
	<td class="row-label" colspan="2">Misc</td>
</tr>
<tr>
	<td class="row-label">Elongation Angle:</td>
	<td class="row-value"><span id="elongation-angle"></span>&deg;</td>
